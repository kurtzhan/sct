<p id="notice"><%= notice %></p>

<h1>Listing Product Attachment Files</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Attachment file</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_attachment_files.each do |product_attachment_file| %>
      <tr>
        <td><%= product_attachment_file.product_id %></td>
        <td><%= product_attachment_file.attachment_file_id %></td>
        <td><%= link_to 'Show', product_attachment_file %></td>
        <td><%= link_to 'Edit', edit_product_attachment_file_path(product_attachment_file) %></td>
        <td><%= link_to 'Destroy', product_attachment_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product attachment file', new_product_attachment_file_path %>
