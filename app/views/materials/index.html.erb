<div class="container">
  <div class="row" style="padding:10px;padding-left:15px;padding-top:1px">
    <div class="panel panel-default">
      <div class="panel-heading" style="background: #cd665f; color: #fff; font-size:13px;">
        Bronze Alloys
      </div>
       <div class="panel-body" style="background: #eeeeee;">
<div class="row">
  <div class="col-md-10">
         <% @materials.each do |m| %>
         <div style="border-left:2px solid #812215;padding-left:10px;margin-top:5px;margin-bottom:10px;">
           <div><strong><%= m.name %></strong></div>
           <div><%= m.description.html_safe if m.description%></div>
           <div style="margin-top:10px;">
           <% m.material_items.each do |mi| %>
             <span><%= link_to mi.name, material_material_item_path(m, mi), {:style => 'color: #792f1f; margin-right:12px', :target => '_blank'} %></span>
           <% end %>
           </div>
         </div>
         <% end %>
  </div>
  <div class="col-md-2" style="padding-left:0px">
    <%= image_tag('right-bar-150.1200.jpg') %>
  </div>
</div>
       </div>
    </div>
  </div>
</div>
